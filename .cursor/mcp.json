{
  "mcpServers": {
  "filesystem": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-filesystem",
  "./"
  ]
  },
  "fetch": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-fetch"
  ]
  },
  "markitdown": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-markitdown"
  ]
  },
  "sqlite": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-sqlite",
  "--db",
  "./app.db"
  ]
  },
  "github": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-github"
  ],
  "env": {
  "GITHUB_PERSONAL_ACCESS_TOKEN": "REPLACE_WITH_YOUR_TOKEN"
  }
  },
  "playwright": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-playwright"
  ]
  },
  "image-processing": {
  "command": "npx",
  "args": [
  "-y",
  "@modelcontextprotocol/server-image-processing"
  ]
  },
  "astro-docs": {
  "url": "https://mcp.docs.astro.build/mcp",
  "transport": "http"
  }
  }
  }